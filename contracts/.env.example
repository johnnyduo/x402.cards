# Example environment variables for deployment
# Copy this to .env and fill in your values

# Private key for deploying contracts (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Optional: Block explorer API key for contract verification
EXPLORER_API_KEY=your_api_key_here

# Fee recipient address (where platform fees are sent)
FEE_RECIPIENT=0x5ebaddf71482d40044391923BE1fC42938129988
